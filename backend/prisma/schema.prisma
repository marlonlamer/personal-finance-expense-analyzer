generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Expense {
  id        Int      @id @default(autoincrement())
  title     String
  amount    Float
  category  String
  createdAt DateTime @default(now())
}

model Income {
  id        Int      @id @default(autoincrement())
  title     String
  amount    Float
  source    String
  createdAt DateTime @default(now())
}


